<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Enables the User to edit the Manual Valid Until field on Quote, which is used to remove the Quote from the Contract Service&apos;s DB at a different date than the calculated Valid Until</description>
    <fieldPermissions>
        <editable>true</editable>
        <field>SBQQ__Quote__c.Manual_Valid_Until__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Quote Edit Manual Valid Until</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SBQQ__Quote__c</object>
        <viewAllFields>false</viewAllFields>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
</PermissionSet>
