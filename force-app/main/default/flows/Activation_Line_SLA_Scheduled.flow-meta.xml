        </rules>
        <rules>
            <name>Planned_Delivery_date_in_5_Days</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SAP_PR_no__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>plannedDeliveryDate</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>5.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Case__r.Type</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>4G</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_SLA_Definition</targetReference>
            </connector>
            <label>Planned Delivery date in 5 Days</label>
        </rules>
    </decisions>
