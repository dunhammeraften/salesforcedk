        <rules>
            <name>Planned_Delivery_Date_is_Today</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.AL_Planned_Delivery_Date__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
