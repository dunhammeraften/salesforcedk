    <formulas>
        <name>DueDateCalculated</name>
        <dataType>Date</dataType>
        <expression>CASE( 
  MOD( {!$Flow.CurrentDate} - DATE( 1900, 1, 7 ), 7 ),
  3, {!$Flow.CurrentDate} + 2 + {!NumberOfWorkDays},
  4, {!$Flow.CurrentDate} + 2 + {!NumberOfWorkDays},
  5, {!$Flow.CurrentDate} + 2 + {!NumberOfWorkDays},
  6, {!$Flow.CurrentDate} + 1 + {!NumberOfWorkDays},
  {!$Flow.CurrentDate} + {!NumberOfWorkDays}
)</expression>
    </formulas>
    <formulas>
