        <dataType>String</dataType>
        <expression>&quot;Number of Connections changed from&quot; + TEXT({!$Record__Prior.Expected_No_of_Connections__c}) + &quot;to&quot; + TEXT({!$Record.Expected_No_of_Connections__c})</expression>
    </formulas>
    <formulas>
        <name>ReminderDate</name>
        <dataType>Date</dataType>
        <expression>{!DueDateCalculated} -1</expression>
