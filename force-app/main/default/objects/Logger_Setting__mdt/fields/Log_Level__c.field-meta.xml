<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Log_Level__c</fullName>
    <description>Definition of the level of logs configured at an Org level. A log level of error will register log entries marked as ERROR, while FINEST will register all types of logs</description>
    <fieldManageability>DeveloperControlled</fieldManageability>
    <label>Log Level</label>
    <required>false</required>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>ERROR</fullName>
                <default>true</default>
                <label>ERROR</label>
            </value>
            <value>
                <fullName>WARN</fullName>
                <default>false</default>
                <label>WARN</label>
            </value>
            <value>
                <fullName>INFO</fullName>
                <default>false</default>
                <label>INFO</label>
            </value>
            <value>
                <fullName>DEBUG</fullName>
                <default>false</default>
                <label>DEBUG</label>
            </value>
            <value>
                <fullName>FINE</fullName>
                <default>false</default>
                <label>FINE</label>
            </value>
            <value>
                <fullName>FINER</fullName>
                <default>false</default>
                <label>FINER</label>
            </value>
            <value>
                <fullName>FINEST</fullName>
                <default>false</default>
                <label>FINEST</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
