<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Non_renewal_pipeline__c</fullName>
    <description>The pipeline value of the non-renewal part of this opportunity.</description>
    <formula>IF(IsClosed = FALSE,
    IF(ISPICKVAL(Type,&quot;Renewal&quot;), SBQQ__PrimaryQuote__r.Cross_and_upsale_amount__c, K12_Amount__c),
    0)
* Probability</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The pipeline value of the non-renewal part of this opportunity.</inlineHelpText>
    <label>Non-renewal pipeline</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
