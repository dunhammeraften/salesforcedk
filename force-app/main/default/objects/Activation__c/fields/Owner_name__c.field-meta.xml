<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Owner_name__c</fullName>
    <description>A concatenation of the first and last name of the owner. Used to save column space in list views</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(Owner__r.Id)),Owner__r.FirstName +&quot; &quot;+ Owner__r.LastName, &quot;&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Owner name</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
